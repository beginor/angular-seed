{
  "name": "ng-seed",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "build": "npm run build-shared && npm run build-web && npm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web": "ng build web --configuration production",
    "build-handset": "ng build handset --configuration production",
    "watch-web": "ng build web --watch --configuration development",
    "watch-handset": "ng build handset --watch --configuration development",
    "lint-shared": "ng lint app-shared",
    "lint-web": "ng lint web",
    "lint-handset": "ng lint handset",
    "start-web": "ng serve web",
    "start-handset": "ng serve handset",
    "test-web": "ng test web --watch",
    "test-handset": "ng test handset --watch",
    "test-web-coverage": "ng test web --code-coverage",
    "test-handset-coverage": "ng test handset --code-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/ng-seed.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/ng-seed/issues"
  },
  "homepage": "https://github.com/beginor/ng-seed#readme",
  "dependencies": {
    "@angular/animations": "~12.0.4",
    "@angular/common": "~12.0.4",
    "@angular/compiler": "~12.0.4",
    "@angular/core": "~12.0.4",
    "@angular/forms": "~12.0.4",
    "@angular/platform-browser": "~12.0.4",
    "@angular/platform-browser-dynamic": "~12.0.4",
    "@angular/router": "~12.0.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4",
    "@angular/localize": "~12.0.4",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.5.0",
    "@ng-bootstrap/ng-bootstrap": "^9.1.2",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@angular/cdk": "~12.0.4",
    "@angular/material": "~12.0.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.6.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "typescript": "~4.2.3"
  }
}
